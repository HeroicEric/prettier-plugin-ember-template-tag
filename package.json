    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@types/estree": "^1.0.0",
    "@types/prettier": "^2.7.1",
    "@vitest/ui": "^0.24.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "ember-cli-htmlbars": "^6.1.1",
    "ember-template-imports": "^3.1.2",
    "prettier": "^2.7.1"
  }
}
