{
  "name": "prettier-plugin-ember-template-tag",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/prettier-plugin-ember-template-tag.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "example": "vite build && ./node_modules/.bin/prettier --plugin . example.gjs > temp.gjs",
    "example-ts": "vite build && ./node_modules/.bin/prettier --plugin . example.gts",
    "lint:eslint": "eslint . --cache",
    "lint:eslint:fix": "eslint . --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@babel/types": "^7.20.0",
    "@types/babel__core": "^7.1.19",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitest/ui": "^0.24.5",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.6.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-template-imports": "^3.3.1",
    "prettier": "^2.7.1"
  }
}
