{
  "name": "prettier-plugin-ember-template-tag",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/prettier-plugin-ember-template-tag.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "example": "vite build && ./node_modules/.bin/prettier --plugin . example.gjs",
